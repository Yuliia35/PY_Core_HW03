from datetime import datetime, timedelta
from random import randint, sample
import re
 
"Excesize 1"

def get_days_from_today(date_string):
    date_string = "2020.10.09"
    string_to_date = datetime.strptime(date_string, "%Y.%m.%d").date()
    today_date = datetime.today().date()
    diff_date = today_date - string_to_date
    
    return diff_date.days
    
print(get_days_from_today("2020.10.11"))

    
"Excesize 2"

def get_numbers_ticket (min_value, max_value, quantity):
    lottery_numbers = set ()
    while len (lottery_numbers) != quantity:
        lottery_numbers.add(randint(min_value, max_value))
    return(sorted(sample(list(lottery_numbers), k=quantity)))
    
print (get_numbers_ticket(1,1000,10))  